{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": false,
		"removeComments": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"target": "es2017",
		"baseUrl": "./client/source",
		"sourceMap": true,
		"inlineSources": true,
		"paths": {
			"components/*": [
				"components"
			],
			"reducers/*": [
				"redux/reducers"
			]
		}
	},
	"include": ["client/source"],
	"exclude": [
		"node_modules"
	]
}